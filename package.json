{
  "name": "bootstrap-v5",
  "version": "1.0.0",
  "description": "bootstrap with sass",
  "main": "index.js",
  "scripts": {
    "sass": "sass --watch scss:css",
    "start": "live-server --open=.",
    "build": "sass scss:css --style compressed",
    "clean": "rimraf css/*.css",
    "dev": "npm-run-all --parallel sass start"
  },
  "keywords": [
    "Bootstrap",
    "bootstrap-v5",
    "sass",
    "learn-bootstrap",
    "learn-sass"
  ],
  "author": "Ravand",
  "license": "ISC",
  "devDependencies": {
    "sass": "^1.78.0",
    "rimraf": "^3.0.2",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "live-server": "^1.2.2"
  }
}
